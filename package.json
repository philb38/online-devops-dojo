    "smee-client": ">=1.1.0",
    "standard": ">=14.3.3"
  },
  "engines": {
    "node": ">= 8.3.0",
    "npm": ">6.0.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
